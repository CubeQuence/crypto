{
  "name": "cubequence/crypto",
  "description": "Cryptography package used in CubeQuence",
  "keywords": [
    "crypto",
    "cubequence"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Luca Castelnuovo",
      "email": "luca@castelnuovo.xyz"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "paragonie/halite": "^4.8",
    "paragonie/paseto": "^2.4"
  },
  "require-dev": {
    "nunomaduro/phpinsights": "dev-master",
    "pestphp/pest": "^1.0"
  },
  "scripts": {
    "tests": "./vendor/bin/pest"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CQ\\Crypto\\": "src/Crypto"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/CubeQuence/crypto/issues",
    "source": "https://github.com/CubeQuence/crypto"
  }
}
